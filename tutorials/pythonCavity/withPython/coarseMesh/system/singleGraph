            (0.0175 0.1 0.005)
            (0.0225 0.1 0.005)
            (0.0275 0.1 0.005)
            (0.0325 0.1 0.005)
            (0.0375 0.1 0.005)
            (0.0425 0.1 0.005)
            (0.0475 0.1 0.005)
            (0.0525 0.1 0.005)
            (0.0575 0.1 0.005)
            (0.0625 0.1 0.005)
            (0.0675 0.1 0.005)
            (0.0725 0.1 0.005)
            (0.0775 0.1 0.005)
            (0.0825 0.1 0.005)
            (0.0875 0.1 0.005)
            (0.0925 0.1 0.005)
            (0.0975 0.1 0.005)
        );
        maxDistance 0.001;
        axis    x;
    }

    // Must be last entry
